{% extends 'base.html.twig' %}

{% block body %}

    <div class="container mt-5">
        {{ form_start(form) }}
        {{ form_row(form.firstname) }}
        {{ form_row(form.lastname) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}

        <button type="submit" class="btn btn-primary">Register!</button>
        {{ form_end(form) }}
    </div>


{% endblock %}
